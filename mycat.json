{
 "id": "mycat",
 "cmd": "/usr/local/startup.sh",
 "mem": 1024,
 "cpus": 1,
 "instances": 1,
 "container": {
   "type": "DOCKER",
   "docker": {
     "image": "registry.super.com/justep/mycat",
     "network": "BRIDGE",
     "portMappings": [
        { "containerPort": 8066, "hostPort": 0, "protocol": "tcp" },
        { "containerPort": 9066, "hostPort": 1, "protocol": "tcp" }
     ],
    "forcePullImage":true
   },
   "volumes": [
    ]
 },
 "env": {
   "SERVICE_TAGS" : "haproxy,public,http",
   "SERVICE_NAME" : "mycat"
 },
 "uris": [],
 "healthChecks": [
   {
     "portIndex": 0,
     "protocol": "TCP",
     "gracePeriodSeconds": 30,
     "intervalSeconds": 10,
     "timeoutSeconds": 30,
     "maxConsecutiveFailures": 3
   },
   {
         "path": "/",
         "portIndex": 0,
         "protocol": "HTTP",
         "gracePeriodSeconds": 30,
         "intervalSeconds": 10,
         "timeoutSeconds": 30,
         "maxConsecutiveFailures": 3
   }
 ],
 "upgradeStrategy": {
    "minimumHealthCapacity": 0.0,
    "maximumOverCapacity": 0.0
  }
}
